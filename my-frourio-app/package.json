{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "run-p dev:*",
    "dev:client": "next dev -p 8000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "dev:pathpida": "pathpida --enableStatic --watch",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:pathpida": "pathpida --enableStatic",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.{ts,js,tsx}\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.{ts,js,tsx}\"",
    "migration:generate": "npm run migration:generate --prefix server",
    "migration:run": "npm run migration:run --prefix server",
    "migration:revert": "npm run migration:revert --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.11.0",
    "@aspida/swr": "^1.11.0",
    "axios": "^0.27.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "frourio": "^0.29.0",
    "next": "^12.2.5",
    "pathpida": "^0.18.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "swr": "^1.3.0",
    "typeorm": "^0.2.41",
    "typeormer": "^0.1.3"
  },
  "devDependencies": {
    "@fastify/cors": "^7.0.0",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.15",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "babel-jest": "^27.5.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "fastify": "^3.27.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.9.1",
    "typescript": "4.7.4"
  }
}
